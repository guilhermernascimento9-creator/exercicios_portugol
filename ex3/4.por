programa {
  funcao inicio() {
    inteiro banco
    escreva("Digite o valor da conta bancária:")
    leia(banco)

    inteiro dolar 
    escreva("Digit o valor do dolare: ")
    leia(dolar)

    real converte
    converte = banco / dolar
    escreva("O valor da sua conta em dolar é: ", converte)


  }
}
