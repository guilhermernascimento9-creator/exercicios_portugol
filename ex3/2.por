programa {
  funcao inicio() {
    inteiro variavel1
    escreva("Digite o primeiro número: ")
    leia(variavel1)
    escreva("Digite o segundo número: ")
    inteiro variavel2
    leia(variavel2)
    inteiro soma = variavel1 + variavel2
    inteiro sub = variavel1 - variavel2
    inteiro multi = variavel1 * variavel2
    inteiro divi = variavel1 / variavel2
   
    escreva("Soma: ", soma ," Subtração: ", sub , " multiplicação: ", multi ," Divisão: " , divi)



  }
}
